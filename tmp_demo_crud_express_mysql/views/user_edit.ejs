<% include partials/_header%>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5">
      <div class="card">
        <div class="card-body">
          <form action="/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input
                type="text"
                name="name"
                value="<%= data.name %>"
                class="form-control"
                required
              />
            </div>
            <div class="form-group">
              <input
                type="text"
                name="address"
                value="<%= data.address %>"
                class="form-control"
                required
              />
            </div>
            <div class="form-group">
              <input
                type="text"
                name="phone"
                value="<%= data.phone %>"
                class="form-control"
                required
              />
            </div>
            <button type="submit" class="btn btn-outline-success">
              Edit User
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer%>
